      "@models/*": ["src/models/*"],
      "@utils/*": ["src/utils/*"],
      "@types": ["src/types/"],
      "@types/*": ["src/types/*"],
      "@constants": ["src/constants/"],
      "@constants/*": ["src/constants/*"],
      "@hooks": ["src/hooks/"],
      "@hooks/*": ["src/hooks/*"],
      "@features": ["src/features/"],
      "@features/*": ["src/features/*"],
      "@redux": ["src/redux/"],
      "@redux/*": ["src/redux/*"],
      "@permissions": ["src/permissions/"],
      "@permissions/*": ["src/permissions/*"],
      "@store": ["src/store/"],
      "@store/*": ["src/store/*"],
      "@telemetry": ["src/telemetry/"],
      "@telemetry/*": ["src/telemetry/*"]
    }
  }
}
