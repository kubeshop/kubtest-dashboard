{
  "compilerOptions": {
    "baseUrl": "./",
    "target": "ESNext",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "strict": true,
    "paths": {
      "@assets": ["src/assets/images/"],
      "@assets/*": ["src/assets/images/*"],
      "@atoms": ["src/components/atoms/"],
      "@atoms/*": ["src/components/atoms/*"],
      "@constants": ["src/constants/"],
      "@constants/*": ["src/constants/*"],
      "@custom-antd": ["src/components/custom-antd"],
      "@custom-antd/*": ["src/components/custom-antd/*"],
      "@env": ["src/env"],
      "@feature-flags": ["src/feature-flags/"],
      "@feature-flags/*": ["src/feature-flags/*"],
      "@hooks": ["src/hooks/"],
      "@hooks/*": ["src/hooks/*"],
      "@icons": ["src/assets/icons/"],
      "@icons/*": ["src/assets/icons/*"],
      "@modal": ["src/modal/"],
      "@modal/*": ["src/modal/*"],
      "@models": ["src/models/"],
      "@models/*": ["src/models/*"],
      "@molecules": ["src/components/molecules/"],
      "@molecules/*": ["src/components/molecules/*"],
      "@organisms": ["src/components/organisms/"],
      "@organisms/*": ["src/components/organisms/*"],
      "@pages": ["src/components/pages/"],
      "@pages/*": ["src/components/pages/*"],
      "@permissions": ["src/permissions/"],
      "@permissions/*": ["src/permissions/*"],
      "@plugins/*": ["src/plugins/*"],
      "@redux": ["src/redux/"],
      "@redux/*": ["src/redux/*"],
      "@services": ["src/services/"],
      "@services/*": ["src/services/*"],
      "@store": ["src/store/"],
      "@store/*": ["src/store/*"],
      "@src/*": ["src/*"],
      "@styles/*": ["src/styles/*"],
      "@telemetry": ["src/telemetry/"],
      "@telemetry/*": ["src/telemetry/*"],
      "@utils": ["src/utils/"],
      "@utils/*": ["src/utils/*"],
      "@testkube/web/src/legacyOssOnly": ["src/legacyOssOnly"],
      "@testkube/web/src/legacyCloudOnly": ["src/legacyCloudOnly"],
    }
  },
  "include": [
    "src"
  ]
}
