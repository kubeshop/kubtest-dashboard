{
  "name": "testkube-dashboard",
  "version": "0.7.0",
  "author": "Kubeshop",
  "description": "Simple dashboard for TestKube",
  "contributors": [
    "ole@kubeshop.io"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:kubeshop/testkube-dashboard.git"
  },
  "private": true,
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@craco/craco": "6.3.0",
    "@testing-library/jest-dom": "5.15.1",
    "@testing-library/react": "12.1.2",
    "@testing-library/user-event": "13.5.0",
    "@trivago/prettier-plugin-sort-imports": "3.1.0",
    "@types/jest": "27.0.2",
    "@types/node": "16.11.10",
    "@types/react": "17.0.30",
    "@types/react-dom": "17.0.11",
    "@types/react-router-dom": "^5.3.1",
    "@types/redux-logger": "^3.0.9",
    "@types/styled-components": "5.1.18",
    "concurrently": "^6.5.0",
    "craco-alias": "3.0.1",
    "craco-less": "1.20.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.27.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-unused-imports": "1.1.3",
    "file-loader": "6.2.0",
    "husky": "7.0.4",
    "json-server": "^0.17.0",
    "prettier": "2.4.1",
    "react-scripts": "4.0.3",
    "stylelint": "^14.2.0",
    "stylelint-config-recommended": "^6.0.0",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript": "4.5.4"
  },
  "dependencies": {
    "@ant-design/charts": "^1.2.14",
    "@ant-design/icons": "4.7.0",
    "@reduxjs/toolkit": "^1.6.2",
    "antd": "4.16.13",
    "loglevel": "1.8.0",
    "moment": "^2.29.1",
    "notyf": "^3.10.0",
    "query-string": "^7.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^5.3.0",
    "react-use": "^17.3.1",
    "styled-components": "5.3.3",
    "web-vitals": "2.1.2",
    "redux-logger": "3.0.6"
  },
  "scripts": {
    "mock-api": "json-server --watch --routes routes.json src/assets/mockedData/executions.json --port 4000",
    "start": "chmod +x scripts/env.sh && scripts/env.sh && cp env-config.js ./public/ && craco start",
    "start:mock-app": "concurrently \"npm run mock-api\" \"npm start\"",
    "build": "CI=false && craco --max_old_space_size=4096 build",
    "test": "craco test --watchAll=false",
    "test:watch": "jest  --watch --detectOpenHandles",
    "test:coverage": "jest -- --coverage --watchAll=false",
    "test:update": "jest  --updateSnapshot",
    "eject": "react-scripts eject",
    "lint": "npm run lint:config && npm run lint:src ",
    "lint:config": "eslint .",
    "lint:src": "eslint \"{src,test}/**/*.{js,jsx,ts,tsx}\"",
    "lintfix": "eslint --fix \"{src,test}/**/*.{js,jsx,ts,tsx}\"",
    "stylelint": "stylelint  \"./src/**/*.tsx\"",
    "precommit": "npm run lint",
    "format:all": "prettier --write \"{src,test}/**/*.{js,jsx,ts,tsx}\"",
    "prepare": "husky install",
    "release:patch": "standard-version --release-as patch",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major"
  },
  "lint-staged": {
    "\"{src,test}/**/*.{js,jsx,ts,tsx}\"": [
      "eslint \"{src,test}/**/*.{js,jsx,ts,tsx}\" --fix",
      "git add"
    ],
    ".": [
      "eslint . --fix",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  }
}
