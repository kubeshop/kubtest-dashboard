{
  "importOrder": [
    "^@kubernetes?.+$",
    "^react?.+$",
    "^antd?.+$",
    "^@ant-design?.+$",
    "^@reduxjs?.+$",
    "^@n8tb1t",
    "^@rjsf?.+$",
    "^(?![@\\.]).+$",
    "^@constants",
    "^@models",
    "^@redux",
    "^@atoms",
    "^@custom-antd",
    "^@molecules",
    "^@organisms",
    "^@pages",
    "^@hooks",
    "^@utils",
    "^@assets",
    "^@icons",
    "^@segment",
    "^@services",
    "^@styles",
    "^@types",
    "^@permissions",
    "^@contexts",
    "^@src",
    "^(?!\\.+\\/styled)^[./]?.+$",
    "^\\.+\\/styled$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "singleQuote": true,
  "arrowParens": "avoid",
  "bracketSpacing": false,
  "semi": true,
  "trailingComma": "es5"
}
